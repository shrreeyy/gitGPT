<% if @responses.any? %>
<div class="container">
  <table class="table table-striped">
    <tr>
      <th>Name of Repository</th>
      <th>Actions</th>
    </tr>
    <% @responses.each do |response|%>
    <tr>
      <td><%= response['name'] %></td>
      <td>Button to link to view Pull requests for this Repositories</td>
    </tr>
    <% end %>
  </table>
</div>
<% else %>
  <p> No repositories found for this Github Account</p>
<% end %>
